{
    "name": "framer-motion--monorepo",
    "version": "0.0.0",
    "private": true,
    "author": "Framer",
    "license": "MIT",
    "workspaces": [
        "packages/*",
        "dev"
    ],
    "resolutions": {
        "webpack": "^5.27.2",
        "scheduler": "^0.23.0"
    },
    "scripts": {
        "build": "turbo run build",
        "dev-projection": "python -m SimpleHTTPServer",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "test-e2e": "turbo run test-e2e",
        "test-ci": "turbo run test-ci --no-cache",
        "collect-projection-tests": "node ./dev/projection/collect-projection-tests.js",
        "start-dev-server": "webpack serve --config dev/webpack/config.js --hot",
        "measure": "turbo run measure",
        "prepare": "turbo run build measure test test-e2e",
        "new": "lerna publish from-package",
        "new-alpha": "npm run prepare && lerna publish from-package --canary --preid alpha",
        "new-beta": "npm run prepare && lerna publish from-package --canary --preid beta"
    },
    "devDependencies": {
        "@cypress/webpack-preprocessor": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.3.0",
        "@rollup/plugin-replace": "^2.3.2",
        "@testing-library/dom": "^8.16.0",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.3.0",
        "@testing-library/react-hooks": "^8.0.1",
        "@types/jest": "^28.1.6",
        "@types/node": "^18.6.3",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@types/styled-components": "^5.1.25",
        "@types/three": "^0.143.0",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "bundlesize": "^0.18.1",
        "concurrently": "^7.3.0",
        "convert-tsconfig-paths-to-webpack-aliases": "^0.9.2",
        "cypress": "^3.4.0",
        "eslint": "^8.21.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^28.0.0",
        "jest-environment-jsdom": "^28.1.3",
        "jest-watch-typeahead": "^2.0.0",
        "lerna": "^4.0.0",
        "lint-staged": "^8.0.4",
        "prettier": "^2.5.1",
        "react": "^18.2.0",
        "react-dev-utils": "^12.0.1",
        "react-dom": "^18.2.0",
        "rollup": "^2.63.0",
        "rollup-plugin-dts": "^4.2.2",
        "rollup-plugin-terser": "^5.3.0",
        "rollup-plugin-visualizer": "^2.4.3",
        "ts-jest": "^28.0.7",
        "ts-loader": "^9.3.1",
        "turbo": "latest",
        "type-fest": "^1.0.1",
        "typescript": "^4.2.3",
        "webpack": "^5.27.2",
        "yarn-deduplicate": "^1.1.1"
    },
    "packageManager": "yarn@3.1.0",
    "dependencies": {
        "start-server-and-test": "^1.14.0"
    }
}
