{
    "name": "framer-motion--monorepo",
    "version": "0.0.0",
    "private": true,
    "author": "Framer",
    "license": "MIT",
    "workspaces": [
        "packages/*",
        "dev"
    ],
    "resolutions": {
        "@react-three/fiber": "7.0.27",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.11",
        "typescript": "^4.2.3",
        "jest": "^24.8.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "webpack": "^5.27.2",
        "three": ">=0.133"
    },
    "scripts": {
        "build": "turbo run build",
        "dev-projection": "python -m SimpleHTTPServer",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "test-e2e": "turbo run test-e2e",
        "test-ci": "turbo run test-ci --no-cache",
        "collect-projection-tests": "node ./dev/projection/collect-projection-tests.js",
        "start-dev-server": "webpack serve --config dev/webpack/config.js --hot",
        "measure": "turbo run measure",
        "prepare": "turbo run build measure test test-e2e",
        "new": "lerna publish from-package",
        "new-alpha": "npm run prepare && lerna publish from-package --canary --preid alpha",
        "new-beta": "npm run prepare && lerna publish from-package --canary --preid beta"
    },
    "devDependencies": {
        "@cypress/webpack-preprocessor": "^4.1.0",
        "@rollup/plugin-node-resolve": "^8.0.0",
        "@rollup/plugin-replace": "^2.3.2",
        "@testing-library/dom": "^6.10.1",
        "@testing-library/react": "13.0.0-alpha.5",
        "@testing-library/react-hooks": "8.0.0-alpha.1",
        "@types/jest": "^23.3.9",
        "@types/node": "^17.0.8",
        "@types/react": "^17.0.39",
        "@types/react-dom": "^17.0.11",
        "@types/styled-components": "^4.1.0",
        "@types/three": "^0.136.1",
        "@typescript-eslint/eslint-plugin": "^5.9.1",
        "@typescript-eslint/parser": "^5.9.1",
        "bundlesize": "^0.18.0",
        "concurrently": "^7.0.0",
        "convert-tsconfig-paths-to-webpack-aliases": "^0.9.2",
        "cypress": "^3.4.0",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "jest": "^27.4.7",
        "jest-dom": "^3.5.0",
        "jest-junit": "^6.4.0",
        "jest-watch-typeahead": "^0.4.2",
        "lerna": "^4.0.0",
        "lint-staged": "^8.0.4",
        "prettier": "^2.5.1",
        "react": "^17.0.2",
        "react-dev-utils": "^11.0.4",
        "react-dom": "^17.0.2",
        "rollup": "^2.63.0",
        "rollup-plugin-dts": "^4.2.2",
        "rollup-plugin-terser": "^5.3.0",
        "rollup-plugin-visualizer": "^2.4.3",
        "ts-jest": "^24.0.2",
        "ts-loader": "^5.3.0",
        "turbo": "latest",
        "type-fest": "^1.0.1",
        "typescript": "^4.2.3",
        "webpack": "^5.27.2",
        "yarn-deduplicate": "^1.1.1"
    },
    "packageManager": "yarn@3.1.0",
    "dependencies": {
        "start-server-and-test": "^1.14.0"
    }
}
