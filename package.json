        "@typescript-eslint/parser": "^7.4.0",
        "animejs": "^3.2.2",
        "bundlesize": "^0.18.1",
        "concurrently": "^7.3.0",
        "convert-tsconfig-paths-to-webpack-aliases": "^0.9.2",
        "cypress": "4",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
